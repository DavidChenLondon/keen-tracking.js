    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-nyan-reporter": "0.0.60",
    "karma-requirejs": "^0.2.2",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.11",
